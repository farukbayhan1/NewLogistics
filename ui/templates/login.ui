   <property name="styleSheet">
    <string notr="true">QCheckBox {
    spacing: 5px;
    color: rgba(1, 34, 87, 230); /* Logo Mavisi */
    font-weight: bold;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border-radius: 3px;
    border: 2px solid rgba(1, 34, 87, 230); /* Logo Mavisi */
    background-color: white;
}

QCheckBox::indicator:hover {
    border: 2px solid rgba(230, 105, 14, 255); /* Logo Turuncusu */
    background-color: rgba(255, 230, 210, 100);
}

QCheckBox::indicator:checked {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0.5, x2:1, y2:0.5,
        stop:0 rgba(1, 34, 87, 230),       /* Logo Mavisi */
        stop:1 rgba(230, 105, 14, 230)     /* Logo Turuncusu */
    );
    image: url(:/icons/checkmark.png); /* Varsa özel bir tik ikonu */
}

QCheckBox::indicator:checked:hover {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0.5, x2:1, y2:0.5,
        stop:0 rgba(1, 30, 70, 255),        /* Daha koyu mavi tonu */
        stop:1 rgba(255, 130, 30, 255)      /* Parlak turuncu tonu */
    );
}
