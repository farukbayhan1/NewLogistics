   <property name="styleSheet">
    <string notr="true">QPushButton#pushButton {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0.5, x2:1, y2:0.5,
        stop:0 rgba(1, 34, 87, 230),       /* Logo Mavisi */
        stop:1 rgba(230, 105, 14, 230)     /* Logo Turuncusu */
    );
    color: rgba(255, 255, 255, 230);       /* Beyaz yazı */
    border-radius: 5px;
    font-weight: bold;
}

QPushButton#pushButton:hover {
    background-color: qlineargradient(
        spread:pad, x1:0, y1:0.5, x2:1, y2:0.5,
        stop:0 rgba(1, 30, 70, 255),        /* Daha koyu mavi tonu */
        stop:1 rgba(255, 130, 30, 255)      /* Parlak turuncu tonu */
    );
}

QPushButton#pushButton:pressed {
    padding-left: 5px;
    padding-top: 5px;
    background-color: rgba(1, 25, 60, 255); /* Koyu mavi */
}

QPushButton#pushButton_2, #pushButton_3, #pushButton_4, #pushButton_5 {
    background-color: rgba(0, 0, 0, 0);
    color: rgba(1, 34, 87, 255); /* Logo Mavisi */
}

QPushButton#pushButton_2:hover, #pushButton_3:hover, #pushButton_4:hover, #pushButton_5:hover {
    color: rgba(230, 105, 14, 255); /* Logo Turuncusu */
}

QPushButton#pushButton_2:pressed, #pushButton_3:pressed, #pushButton_4:pressed, #pushButton_5:pressed {
    padding-left: 5px;
    padding-top: 5px;
    color: rgba(180, 85, 10, 255); /* Daha koyu turuncu */
}
